        function uploadImageToS3ForSummerNote(file, _ctrl) {
            var url = '@Url.Action("SubirArchivoIMG", "Visita")';

            formData = new FormData();
            formData.append("file", file);
            $.ajax({
                type: 'POST',
                url: url,
                data: formData,
                cache: false,
                contentType: false,
                processData: false,
                success: function (data) {
                    //alert(data.fileUrl);
                    //$('#m_antecedentes').summernote('insertImage', data.fileUrl);
                    var imgNode = document.createElement('img');
                    imgNode.src = data.fileUrl;
                    //$('#m_antecedentes').summernote('insertNode', imgNode);
                    $(_ctrl).summernote('insertNode', imgNode);
                },
                error: function (data) {
                    alert(data.responseText);
                }
            });
        }


        function formatofecha(_fecha) {
            //alert(_fecha);
            var data = _fecha.toString().split('/');
            var d = data[0];
            var m = data[1];
            var y = data[2];

            //alert('dia ' + d);
            //alert('mes ' + m);
            //alert('año ' + y);
            var newfecha = y + '-' + m + '-' + d;
            //alert(newfecha);

            return newfecha;
        }


        function loadCombo(_id, _controlador, _accion, _div, _vacio) {

            $.ajax({
                url:  _controlador + '/' + _accion,
                type: "GET",
                data: {
                },
                contentType: "application/json;charset=utf-8",
                dataType: "json",
                success: function (result) {
                    //console.log(result);

                    //alert(_accion);
                    $('#' + _div).html("");

                    if (_vacio == 1) {
                        if (_id == 0) {
                            $('#' + _div)
                                .append(
                                    $('<option selected="selected" value="0"/>').html("Seleccione")
                                );
                        } else {
                            $('#' + _div)
                                .append(
                                    $('<option value="0"/>').html("Seleccione")
                                );
                        }
                    }

                    $.each(JSON.parse(result), function (key, item) {

                        if (item.id_value == _id) {
                            //alert(1);
                            $('#' + _div)
                                .append(
                                    $('<option selected="selected" value="' + item.id_value + '"/>').html(item.valor_value)
                                );
                        } else {
                            //alert(2);
                            $('#' + _div)
                                .append(
                                    $('<option value="' + item.id_value + '"/>').html(item.valor_value)
                                );
                        }

                    });

                },
                error: function (errormessage) {
                    alert(errormessage.responseText);
                }
            });

        }


